@model IEnumerable<Band.Models.Students>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StudName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudGradYear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudInstType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudInstrument)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StudName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudGradYear)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudInstType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudInstrument)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
